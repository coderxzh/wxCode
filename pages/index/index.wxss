.search{
    width: 100%;
    position: fixed;
    top: 0;
    z-index: 10000;
}
.swiper{
    margin-top: 54px;
    padding: 10px;
}
.swiper image{
    width: 100vw;
    height: 150px;
    border-radius: 10px;   
}
.list {
    height: 200rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 20rpx 0;
    padding: 0 20rpx;
    font-size: 13px;
    text-align: center;
}
.list navigator {
    background-color: #fff;
    opacity: 1;
}
.list view{
    width: 120rpx;
    height: 120rpx;
    margin-bottom: 8px;
    border-radius: 50%;
    background-color: rgba(188, 212, 240, 0.322);
    font-size: 38px !important;
    text-align: center;
    line-height: 120rpx;
    color:#fff;
}
.nav{
    margin-bottom: 60px;
}
.topNews{
    display: flex;
    flex-direction: column;
}
.topNews .top-item{
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 5px;
    padding: 10px;
    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
    border-radius: 4px;
    
}
.topNews .top-item image{
    width:100px;
    height: 60px;
    border-radius: 4px;
}
.topNews .top-item .title{
    width: 200px;
    display: flex;
    flex-direction: column;
    font-size: 14px;
}
.topNews .top-item .title .keywords:hover{
    color:#409EFF;
}
.loading{
    position: fixed;
    bottom: 60px;
    left: 50%;
    transform: translateX(-50%);
}